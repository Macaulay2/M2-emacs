(defconst M2-symbols-version
  "@M2VERSION@"
  "The M2 version used to generate the symbols used by the M2 major modes.")

(defconst M2-symbols-completion-table
  '( @M2SYMBOLS@ )
  "A list of the symbols available in Macaulay2, for use with dynamic completion." )

(defconst M2-symbols-keyword-regexp  (regexp-opt '( @M2KEYWORDS@  ) 'words)
  "Regular expression for Macaulay2 keywords.")
(defconst M2-symbols-type-regexp     (regexp-opt '( @M2DATATYPES@ ) 'words)
  "Regular expression for Macaulay2 types.")
(defconst M2-symbols-function-regexp (regexp-opt '( @M2FUNCTIONS@ ) 'words)
  "Regular expression for Macaulay2 functions.")
(defconst M2-symbols-constant-regexp (regexp-opt '( @M2CONSTANTS@ ) 'words)
  "Regular expression for Macaulay2 constants.")

(provide 'M2-symbols)

