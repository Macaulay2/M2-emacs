;;; M2-symbols.el --- Generated symbols used by the Macaulay2 major modes
;; URL: https://github.com/Macaulay2/M2-emacs
;; Version: 1.25.11

;;; Commentary:

;; This file contains generated data describing Macaulay2 symbols.
;;
;; The variables defined here are used by Macaulay2 major modes to provide
;; symbol completion and syntax highlighting.
;;
;; @M2BANNER@

;;; Code:

;; use this function with C-h f to discover which instance and version of this file is loaded
(defun M2-version () "@M2VERSION@" "The corresponding Macaulay2 version number for the loaded Macaulay2 major mode.")

(defconst M2-symbols-completion-table
  '( @M2SYMBOLS@ )
  "A list of the symbols available in Macaulay2, for use with dynamic completion." )

(defconst M2-symbols-keyword-regexp  (regexp-opt '( @M2KEYWORDS@  ) 'words)
  "Regular expression for Macaulay2 keywords.")
(defconst M2-symbols-type-regexp     (regexp-opt '( @M2DATATYPES@ ) 'words)
  "Regular expression for Macaulay2 types.")
(defconst M2-symbols-function-regexp (regexp-opt '( @M2FUNCTIONS@ ) 'words)
  "Regular expression for Macaulay2 functions.")
(defconst M2-symbols-constant-regexp (regexp-opt '( @M2CONSTANTS@ ) 'words)
  "Regular expression for Macaulay2 constants.")

(provide 'M2-symbols)

;;; M2-symbols.el ends here
